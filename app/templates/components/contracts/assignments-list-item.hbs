<tr class="contract-assignments-list-participant">
  <td>
    {{full-name student}}
  </td>
  {{#each assignments as |assignment|}}
    {{contracts/assignments-list-item-column
      enrollment=enrollment
      assignment=assignment
      turninsHashedByAssignment=turninsHashedByAssignment
    }}
  {{/each}}
</tr>
<tr class="notes-row {{loadingNotes}}">
  <td colspan="3">
    {{notes-list
      notes=notes
      expanded=notesExpanded
    }}
  </td>
</tr>