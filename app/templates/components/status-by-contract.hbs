<thead>
  <tr>
    <th>Name</th>
    <th>Status</th>
    {{#each months as |month|}}
      <th data-test-term-month class="center" title={{t-date month "compactTermMonth"}}>{{t-date month "MMM"}}</th>
    {{/each}}
  </tr>
</thead>
{{#each enrollments as |enrollment|}}
  {{status-by-contract/row
    enrollment=enrollment
    statusHash=statusHash
    notesHash=notesHash
    months=months
  }}
{{/each}}
